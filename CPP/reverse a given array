#include <iostream>

using namespace std;

int main()
{ 
    int A[5]; 
    cout<<"Enter the elements of an array"<<endl;
    for(int i=0;i<5;i++) 
    { 
        cin>>A[i];
    }  
    cout<<"Reverse of a given array is"<<endl;
    for(int i=4;i>=0;i--) 
    { 
        cout<<A[i]<<'\t';
    } 
    return 0;
}
